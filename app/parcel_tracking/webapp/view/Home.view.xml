<mvc:View
    height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:ff="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns:t="sap.ui.table"
    xmlns:trm="sap.ui.table.rowmodes"
    controllerName="parceltracking.controller.Home"
>
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <f:ShellBar
                id="_IDGenShellBar1"
                title="Package Tracking Service"
                showMenuButton="true"
                homeIcon="img/logo.png"
                showCopilot="true"
                showNotifications="true"
                menuButtonPressed="onMenuButtonPress"
                notificationsNumber="2"
            >
                <f:profile>
                    <Avatar
                        id="_IDGenAvatar1"
                        initials="UI"
                    />
                </f:profile>
            </f:ShellBar>
        </tnt:header>
        <tnt:sideContent>
            <core:Fragment
                fragmentName="parceltracking.view.SideNavigation"
                id="fragment"
                type="XML"
            />
        </tnt:sideContent>
        <tnt:mainContents>
            <NavContainer
                id="pageContainer"
                initialPage="Sender_Overview"
            >
                <pages>
                    <ScrollContainer
                        id="Sender_Overview"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                    >
                        <core:Fragment
                            fragmentName="parceltracking.view.SenderOverview"
                            type="XML"
                        />
                    </ScrollContainer>
                    <Page
                        id="Sender_Create"
                        title="{i18n>createTitle}"
                        showNavButton="true"
                        navButtonPress=".onNavBack"
                        enableScrolling="true"
                    >
                        <core:Fragment
                            fragmentName="parceltracking.view.SenderCreate"
                            id="SenderCreateFragment"
                            type="XML"
                        />
                       
                    </Page>
                    <Page
                        id="Sender_Edit"
                        title="{i18n>editTitle}"
                        showNavButton="true"
                        navButtonPress=".onNavBack"
                        enableScrolling="true"
                    >
                        <core:Fragment
                            fragmentName="parceltracking.view.SenderEdit"
                            id="SenderEditFragment"
                            type="XML"
                        />
                        <!-- <footer>
                            <OverflowToolbar id="overflowToolbar124">
                                <content>
                                    <ToolbarSpacer id="toolbarSpacer124" />
                                    <Button
                                        id="onCancelEdit"
                                        text="Cancel"
                                        press=".SenderEdit.onCancel"
                                        icon="sap-icon://cancel"
                                    />
                                    <Button
                                        id="onSubmitEdit"
                                        text="Submit"
                                        press=".SenderEdit.onSubmit"
                                        icon="sap-icon://complete"
                                    />
                                </content>
                            </OverflowToolbar>
                        </footer> -->
                    </Page>
                      <Page
                        id="Receiver_Overview"
                        title="{i18n>editTitle}"
                        showNavButton="true"
                        navButtonPress=".onNavBack"
                        enableScrolling="true"
                    >
                        <core:Fragment
                                fragmentName="parceltracking.view.ReceiverOverviewContent"
                                      id="ReceiverOverviewFragment"
                                type="XML"
                            />
                
                    </Page>
                     <Page
                        id="Receiver_Detail"
                        title="{i18n>editTitle}"
                        showNavButton="true"
                        navButtonPress=".onNavBack"
                        enableScrolling="true"
                    >
                    <core:Fragment
                                fragmentName="parceltracking.view.ReceiverDetailContent"
                                type="XML"
                            />
                
                    </Page>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>

<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:ff="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
>
    <VBox
        id="_IDGenVBox1"
        fitContainer="true"
    >
        <ff:SimpleForm
            id="SimpleFormDisplay480"
            editable="false"
            layout="ResponsiveGridLayout"
            title="Overview"
            labelSpanL="4"
            labelSpanM="4"
            emptySpanL="0"
            emptySpanM="0"
            columnsL="2"
            columnsM="2"
        >
            <ff:content>
                <core:Title
                    id="_IDGenTitle1"
                    text="Personal Information"
                />
                <Label
                    id="_IDGenLabel1"
                    text="Name"
                />
                <Text
                    id="_IDGenText2"
                    text="Sample User"
                />
                <Label
                    id="_IDGenLabel5"
                    text="Email"
                />
                <Text
                    id="_IDGenText1144"
                    text="test@sample.com"
                />
                <Label
                    id="_IDGenLabel6"
                    text="Phone"
                />
                <Text
                    id="_IDGenText1232"
                    text="+(60) 1234-5678"
                />
                <core:Title
                    id="_IDGenTitle2"
                    text="Address"
                />
                <Label
                    id="_IDGenLabel2"
                    text="Street/No."
                />
                <Text
                    id="_IDGenText3"
                    text="Main St 1618"
                />
                <Label
                    id="_IDGenLabel3"
                    text="ZIP Code/City"
                />
                <Text
                    id="_IDGenText4"
                    text="31415 Maintown"
                />
                <Label
                    id="_IDGenLabel4"
                    text="Country"
                />
                <Text
                    id="_IDGenText5"
                    text="Germany"
                />
            </ff:content>
            <ff:layoutData>
                <FlexItemData
                    id="_IDGenFlexItemData1"
                    shrinkFactor="0"
                    backgroundDesign="Solid"
                    styleClass="sapContrastPlus"
                />
            </ff:layoutData>
        </ff:SimpleForm>
        <Table
            id="idProductsTable"
            items="{
        path: '/Packages',
        filters: [{
            path: 'shippingStatus',
            operator: 'NE',
            value1: 'NEW' 
        }]}"
        >
            <headerToolbar>
                <OverflowToolbar id="_IDGenOverflowToolbar1">
                    <Title
                        id="_IDGenTitle3"
                        text="Packages"
                        level="H2"
                    />
                    <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                    <Button
                        id="_IDGenButton1"
                        tooltip="Sort"
                        icon="sap-icon://sort"
                        press=".ReceiverOverview.handleSortButtonPressed"
                    />
                    <Button
                        id="_IDGenButton2"
                        tooltip="Filter"
                        icon="sap-icon://filter"
                        press=".ReceiverOverview.handleFilterButtonPressed"
                        visible="false"
                    />
                    <Button
                        id="_IDGenButton3"
                        tooltip="Group"
                        icon="sap-icon://group-2"
                        press=".ReceiverOverview.handleGroupButtonPressed"
                    />
                    <SearchField
                        id="searchField"
                        width="auto"
                        liveChange=".ReceiverOverview.onFilterItems"
                    />
                </OverflowToolbar>
            </headerToolbar>
            <infoToolbar>
                <OverflowToolbar
                    id="vsdFilterBar"
                    visible="false"
                >
                    <Text id="vsdFilterLabel" />
                </OverflowToolbar>
            </infoToolbar>
            <columns>
                <Column
                    id="product"
                    minScreenWidth="Tablet"
                     demandPopin="true"
                >
                    <Text
                        id="_IDGenText8"
                        text="{i18n>packageNumberLabel}"
                    />
                </Column>
                <Column
                    id="_IDGenColumn1"
                    minScreenWidth="Tablet"
                    demandPopin="true"
                >
                    <Text
                        id="_IDGenText9"
                        text="{i18n>shippingStatusLabel}"
                    />
                </Column>
                <Column
                    id="_IDGenColumn14"
                    minScreenWidth="Tablet"
                    demandPopin="false"
                >
                    <Text
                        id="_IDGenText91"
                        text="{i18n>packageStatusLabel}"
                    />
                </Column>
                <Column
                    id="_IDGenColumn2"
                    minScreenWidth="Tablet"
                    demandPopin="true"
                >
                    <Text
                        id="_IDGenText10"
                        text="{i18n>packageWeightLabel}"
                    />
                </Column>
                <Column
                    id="_IDGenColumn3"
                    minScreenWidth="Tablet"
                    demandPopin="true"
                >
                    <Text
                        id="_IDGenText11"
                        text="{i18n>packageHeightLabel}"
                    />
                </Column>
                <Column
                    id="_IDGenColumn4"
                    minScreenWidth="Tablet"
                    demandPopin="true"
                >
                    <Text
                        id="_IDGenText12"
                        text="{i18n>packageCreationDate}"
                    />
                </Column>
                <!-- <Column
                    id="_IDGenColumn5"
                    minScreenWidth="Tablet"
                    demandPopin="true"
                >
                    <Text
                        id="_IDGenText19"
                        text="Action"
                    />
                </Column> -->
            </columns>
            <items>
                <ColumnListItem
                    id="_IDGenColumnListItem1"
                    vAlign="Middle"
                    press=".ReceiverOverview.onEditButtonPress"
                    type="Active"
                >
                    <cells>
                        <ObjectIdentifier
                            id="_IDGenObjectIdentifier1"
                            text="{packageNumber}"
                            class="green"
                        />
                        <tnt:InfoLabel
                            id="_IDGenInfoLabel1"
                            text="{path: 'shippingStatus', formatter: '.ReceiverOverview.textFormatter', targetType: 'any'}"
                            colorScheme="{path: 'shippingStatus', formatter: '.ReceiverOverview.availableState', targetType: 'any'}"
                        />
                        <tnt:InfoLabel
                            id="_IDGenInfoLabel2"
                            text="{path: 'packageStatus', formatter: '.ReceiverOverview.textFormatter', targetType: 'any'}"
                            colorScheme="{path: 'packageStatus', formatter: '.ReceiverOverview.availableState', targetType: 'any'}"
                        />
                        <Text
                            id="_IDGenText14"
                            text="{weight}"
                        />
                        <Text
                            id="_IDGenObjectNumber1"
                            text="{height}"
                        />
                        <Text
                            id="_IDGenObjectNumber2"
                            text="{createdAt}"
                        />
                        <!-- <core:Icon
                            id="_IDGenIcon1"
                            src="sap-icon://edit"
                            class="IconStyle"
                            press="onEditButtonPress"
                        /> -->
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </VBox>
</core:FragmentDefinition>

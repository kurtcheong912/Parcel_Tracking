<mvc:View
    height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:ff="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns:t="sap.ui.table"
    xmlns:trm="sap.ui.table.rowmodes"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    controllerName="parceltracking.controller.ReceiverDetail"
>
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <f:ShellBar
                id="_IDGenShellBar1"
                title="Package Tracking Service"
                showMenuButton="true"
                homeIcon="img/logo.png"
                showCopilot="true"
                showNotifications="true"
                menuButtonPressed="onMenuButtonPress"
                notificationsNumber="2"
            >
                <f:menu>
                    <Menu id="_IDGenMenu1">
                        <MenuItem
                            id="_IDGenMenuItem1"
                            text="Flight booking"
                            icon="sap-icon://flight"
                        />
                        <MenuItem
                            id="_IDGenMenuItem2"
                            text="Car rental"
                            icon="sap-icon://car-rental"
                        />
                    </Menu>
                </f:menu>
                <f:profile>
                    <Avatar
                        id="_IDGenAvatar1"
                        initials="UI"
                    />
                </f:profile>
            </f:ShellBar>
        </tnt:header>
        <tnt:sideContent>
            <core:Fragment
                fragmentName="parceltracking.view.SideNavigation"
                id="fragment"
                type="XML"
            />
        </tnt:sideContent>
        <tnt:mainContents>
            <NavContainer
                id="pageContainer"
                initialPage="Receiver_Detail"
            >
                <pages>

                        <Page
                        id="Sender_Create"
                        title="Package Detail"
                        showNavButton="true"
                        navButtonPress=".onNavBack"
                        enableScrolling="false"
                    >
                                            <content>

                            <core:Fragment
                                fragmentName="parceltracking.view.ReceiverDetailContent"
                                type="XML"
                            />
                                                    </content>
                     <!-- <footer>
                            <OverflowToolbar id="toggleableButtonSection">
                                <content>
                                    <ToolbarSpacer id="toolbarSpacer124" />
                                    <Button
                                        id="onCancelEdit"
                                        text="Order Received"
                                        press="onOrderReceived"
                                        icon="sap-icon://status-completed"
                                        type="Emphasized"
                                    />
                                    <Button
                                        id="onSubmitEdit"
                                        text="Order Damaged"
                                        press="onOrderDamaged"
                                        icon="sap-icon://alert"
                                        type="Critical"
                                    />
                                </content>
                            </OverflowToolbar>
                        </footer> -->
                            </Page>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
